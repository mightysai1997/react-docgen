{
  "compilerOptions": {
    "jsx": "react",
    "declaration": true,
    "lib": ["es2021"],
    "target": "es2021",
    "module": "node16",
    "moduleResolution": "node16",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSourceMap": false,
    "skipLibCheck": true
  }
}
